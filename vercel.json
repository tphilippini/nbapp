{
  "buildCommand": "cd app && npm i && ./node_modules/vite/bin/vite.js build",
  "outputDirectory": "app/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}
